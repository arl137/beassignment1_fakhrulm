<!DOCTYPE html>
<html>
<head>
  <title>Books List</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    table, th, td {
      border: 1px solid black;
    }
    th, td {
      padding: 8px;
      text-align: left;
    }
  </style>
</head>
<body>
  <h1>Books List</h1>
  <table>
    <thead>
      <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Author</th>
        <th>Year Written</th>
        <th>Edition</th>
        <th>Price</th>
      </tr>
    </thead>
    <tbody>
      <% books.forEach(book => { %>
        <tr>
          <td><%= book.id %></td>
          <td><%= book.title %></td>
          <td><%= book.author %></td>
          <td><%= book.year_written %></td>
          <td><%= book.edition %></td>
          <td><%= book.price %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
